%nav.navbar.navbar-default{:role => 'navigation'}
  .container
    = link_to 'JetRuby Lunch', root_path, class: 'navbar-brand'
    #bs-example-navbar-collapse-1.collapse.navbar-collapse
      %ul.nav.navbar-nav.navbar-right
        - if user_signed_in?
          %li= link_to 'Edit Profile', edit_users_profile_path(:user)
          %li= link_to 'Sign Out', destroy_user_session_path, method: :delete
        - else
          %li= link_to 'Sign Up', new_user_registration_path, class: 'active'
          %li= link_to 'Sign In', new_user_session_path, class: 'active'
          %li= link_to 'Login twitter', user_twitter_omniauth_authorize_path(provider: 'twitter'), class: 'active'
          %li= link_to 'Login facebook', user_facebook_omniauth_authorize_path(provider: 'facebook'), class: 'active'
